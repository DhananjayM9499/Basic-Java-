package cdac.in;

import java.util.Iterator;

public class ArrayAdd {

	public static void main(String[] args) {

		int sum = 0;
		int[] num = {10,12,17,18};
		for (int i = 0; i < num.length; i++) {
			
		  
			System.out.print(num[i] + " + ");

			sum = sum + num[i];
		
		
			
		}
		
		System.out.println("");
		System.out.println(sum);

	}

}
