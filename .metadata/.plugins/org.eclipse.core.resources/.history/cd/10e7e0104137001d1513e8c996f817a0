package cdac.in;

public class MaxMin {

	public static void main(String[] args) {
		int[] array = { 1, 5, 8, 6, 7, 6 };
		for (int i = 0; i < array.length; i++) {
			for (int j = 0; j < array.length; j++) {

				if (array[i] > array[j]) {}
				
					

				
			}
System.out.println(array[i]);
		}
	}

}
