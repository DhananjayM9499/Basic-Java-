package cdac.in;

import java.util.*;

public class Student {

	public static void main(String[] args) {
		int sum=0,Average;
		Scanner scan = new Scanner(System.in);
		int[] student1 = new int[5];
		for (int i = 0; i < student1.length; i++) {
			System.out.println("Enter marks of subjects");// NOT COMPLETED
			student1[i] = scan.nextInt();
			if(student1[i]>100) {
				System.out.println("Invalid marks");
			}
			
		}
		for (int i = 0; i < student1.length; i++) {
			
			   sum = sum + student1[i];
			   Average=sum/5;
			  if (sum>=80) {
				  System.out.println("A Grade");
				  break;
			  }
				
			 else if(sum<80&&sum>=60) {
				 System.out.println("B Grade");
				 break;
			 }
			 else if(sum<60&&sum>=40) {
				 System.out.println(" Grade");
				 break;
			 }
			 else {
				 
					 System.out.println("D Grade");
					 break;
				 }
			 }

			}
			  
	//	for (int j = 0; j < student1.length; j++) {
			//System.out.print(student1[j] + " ");

		

	}


