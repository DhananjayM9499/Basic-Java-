package cdac.in;

public class Ascending {

	public static void main(String[] args) {
		int[] array = { 5, 8, 6, 1, 7, 6 };
		int min = array[0];
		for (int i = 1; i < array.length; i++) {
			if (array[i] < min) {
				min = array[i];
				System.out.println(array[i]);

			}
			if(array[i]<min && min>array[i]);

		}

	}

}
