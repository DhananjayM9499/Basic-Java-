package practice.in;
import java.util.*;
public class InnvertedPyramid {

	public static void main(String[] args) {
		int a;
		Scanner scan=new Scanner(System.in);
		System.out.println("enter number of stars");
		a=scan.nextInt();
		for (int i = a; i>= 0; i--) {
			for (int j = i; j>= 0; j--) {
				System.out.print("*");
			}
			
			System.out.print(" ");
		}
	}

}
