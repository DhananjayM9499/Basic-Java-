package cdac.in;

class paraConst {
	int id;
	String name;
	paraConst(int id,String name){
		this.id=id;
		this.name=name;
		
	}
public void display() {
	System.out.println(id);
	System.out.println(name);
}

	}
class ParaConst{
	public static void main(String[] args) {
		paraConst student1=  new paraConst(101,"Danny");
		student1.display();
	}
}

