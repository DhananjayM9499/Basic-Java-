package cdac.in;

public class OddEven {

	public static void main(String[] args) {
		int[] array= {20,21,26,27,66,69};
		for (int i = 0; i < array.length; i++) {
			if (array[i]%2==0) {
				System.out.println(array[i]+" ");
				
			} if(array[i]%2!=0) {
				System.out.println("odd"+array[i]);

			}
		}

	}

}
